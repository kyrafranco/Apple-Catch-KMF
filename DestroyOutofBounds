using UnityEngine;

public class DestroyOutofBounds : MonoBehaviour
{
    private float bottomLimit = -5;
    // Start is called before the first frame update
    void Start()
    {
        if (transform.position.y < bottomLimit)
        {
            Destroy(gameObject);
        }
    }

    // Update is called once per frame
    void Update()
    {
        
    }
}
